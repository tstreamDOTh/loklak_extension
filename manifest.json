{
  "manifest_version": 2,
  "name": "loklak",
  "version": "1.0",

  "background": {
    "scripts": ["eventPage.js"],
    "persistent":false
  },

  "browser_action": {
  "default_popup": "popup.html"
  },

  "icons": {
    "128": "icons/loklak.png",
    "48": "icons/loklak.png",
    "16": "icons/loklak.png"
  },

  "permissions": ["contextMenus"]
}